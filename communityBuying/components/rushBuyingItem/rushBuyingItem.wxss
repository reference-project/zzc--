.add-cart-content{
  display: flex;
  align-items: center;
  padding: 10rpx 0;
  height: 60rpx;
}
.icon-add-cart{
  width: 60rpx;
  height: 60rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background-image: linear-gradient(145deg, #FEE335 0%, #FFBF3A 100%);
  border-radius: 50%;
}
.icon-add-cart image{
  width: 28rpx;
  height: 28rpx;
}
.add-cart-btns{
  display: flex;
  align-items: center;
  justify-content: center;
}
.cart-num{
  width: 46rpx;
  text-align: center;
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #555555;
}
.cart-btn{
  width: 46rpx;
  height: 46rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
  color: #333333;
  font-weight: 400;
  box-sizing: border-box;
  border-radius: 50%;
}
.cart-btn.desc{
  border: 2rpx solid #DDDDDD;
}
.cart-btn.add{
  background-image: linear-gradient(145deg, #FEE335 0%, #FFBF3A 100%);
}
.cart-btn.add.disabled{
  opacity: 0.4;
}

