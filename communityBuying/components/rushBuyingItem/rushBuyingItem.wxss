.pro-desc-tags{
  margin-top: 10rpx;
  white-space: nowrap;
  overflow-x: scroll;
  height: 32rpx;
  font-size: 0px;
  margin-right: 6rpx;
}
.pro-desc-tag{
  display: inline-block;
  padding: 0rpx 13rpx;
  height: 32rpx;
  /* border: 1rpx solid #FCABA7; */
  border-radius: 16rpx;
  margin-left: 8rpx;
  font-family: PingFangSC-Medium;
  font-size: 20rpx;
  color: #FF544C;
  box-sizing: border-box;
  line-height: 30rpx;
}
.pro-desc-tag.active{
  background: #FF544C;
  color: #fff;
  border: 0;
  line-height: 32rpx;
}
.pro-desc-tag.green{
  background-color: #7ED321;
  color: #fff;
  border: 0;
  line-height: 32rpx;
}
.pro-desc-tag.orange{
  background:rgba(255,80,64,1);
  color: #fff;
  border: 0;
  line-height: 32rpx;
}
.pro-desc-tag:first-child{
  margin-left: 0rpx;
}
.pro-info-desc{
  display: inline-block;
  max-width: 380rpx;
  font-family: PingFangSC-Regular;
  font-size:20rpx;
