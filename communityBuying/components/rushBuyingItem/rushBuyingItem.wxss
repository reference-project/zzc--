.pro-price-box{
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.pro-price-new-old{
  display: flex;
  align-items: center;
}
.pro-price-new{
  font-family: PingFangSC-Medium;
  font-size: 24rpx;
  color: #FF5E53;
  /* line-height: 54rpx; */
  display: flex;
  align-items: center;
}
.pro-price-new text{
  font-size: 26rpx;
  /* margin-left: 10rpx; */
  display: block;
}
.pro-price-new text.big-font{
      font-size: 40rpx;
      font-weight: 500;
      margin-bottom: 6rpx;
}
.pro-price-old{
  margin-left: 10rpx;
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #999999;
  text-decoration: line-through;
}
.pro-action{
  width: 160rpx;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  border-radius: 100rpx;
  font-family: PingFangSC-Medium;
  font-size: 26rpx;
  color: #333333;
}
.pro-action.buy{
  background-color: #FF5E53;
}
.pro-action.seed{
  opacity: 0.4;
  background-image: linear-gradient(145deg, #FEE335 0%, #FFBF3A 100%);
}
.pro-action.sellout{
  opacity: 0.4;
  background-image: linear-gradient(145deg, #FEE335 0%, #FFBF3A 100%);
}
.pro-action.next{
  color: #FF5E53;
  background-color:rgba(255, 232, 73, 0.4);
}
