}
.inner{
  text-align: center;
}
.inner-text-en{
  margin-top: 10rpx;
}


.pro-info-content{
  margin-left: 30rpx;
  flex: 1;
  width: 1rpx;
  display: flex;
  flex-direction: column;
  padding: 8rpx 0;
}
.pro-info-top{
  flex: 1;
  font-family: PingFangSC-Medium;
  font-size: 30rpx;
  color: #333333;
}
.pro-info-title{
  max-height: 92rpx;
  line-height: 46rpx;
  font-size: 30rpx;
}
.pro-info-title text{
  vertical-align: middle;
  line-height: 34rpx;
}
.two-ellipsis{
  display: -webkit-box;
  overflow: hidden;
  -webkit-line-clamp:2;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
}

.ellipsis{
    text-overflow:ellipsis; 
    white-space:nowrap;
   overflow:hidden;
}


.pro-tips{
  display: inline-block;
  width: auto;
  height: 34rpx;
  margin-right: 10rpx;
  border-radius: 8rpx;
  text-align: center;
}
.pro-tips.limit{
  padding: 0 4rpx;
  border: 2rpx solid #FF5E53;
  box-sizing: border-box;
  font-family: PingFangSC-Regular;
  font-size: 20rpx;
  color: #FF5E53;
  line-height: 30rpx;
}
.pro-tips.booking{
  font-family: PingFangSC-Medium;
  font-size: 22rpx;
  background-color: #fde7e5;
  width: 74rpx;
  line-height: 34rpx;
  color: #fd6058;
}
